<%= form_tag students_path, method: :get do %>
    <%= text_field_tag :query, params[:query], placeholder: "Search Students" %>
    <%= submit_tag "Search" %>
  <% end %>
  
  <h3>Search Result</h3>

  <% @students.each do |s| %>
    <%= render partial: 'student', locals: {student: s} %>
  <% end %> 
 